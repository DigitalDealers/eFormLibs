{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "@didi/auth": [
        "dist/auth"
      ],
      "@didi/auth/*": [
        "dist/auth/*"
      ],
      "@didi/firebase-api": [
        "dist/firebase-api"
      ],
      "@didi/firebase-api/*": [
        "dist/firebase-api/*"
      ],
      "@didi/geostore": [
        "dist/geostore"
      ],
      "@didi/geostore/*": [
        "dist/geostore/*"
      ],
      "@didi/interceptors": [
        "dist/interceptors"
      ],
      "@didi/interceptors/*": [
        "dist/interceptors/*"
      ],
      "@didi/batch-api": [
        "dist/batch-api"
      ],
      "@didi/batch-api/*": [
        "dist/batch-api/*"
      ],
      "@didi/dealer-api": [
        "dist/dealer-api"
      ],
      "@didi/dealer-api/*": [
        "dist/dealer-api/*"
      ],
      "@didi/safety-api": [
        "dist/safety-api"
      ],
      "@didi/safety-api/*": [
        "dist/safety-api/*"
      ],
      "@didi/table": [
        "dist/table"
      ],
      "@didi/table/*": [
        "dist/table/*"
      ]
    }
  }
}
