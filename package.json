{
  "name": "didi-libs",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "lib:auth-api:build": "ng-packagr -p libs/auth-api",
    "lib:auth-api:publish": "npm publish ./dist/auth-api --access=public",
    "lib:auth:build": "ng-packagr -p libs/auth",
    "lib:auth:publish": "npm publish ./dist/auth --access=public",
    "lib:base-component:build": "ng-packagr -p libs/base-component",
    "lib:base-component:publish": "npm publish ./dist/base-component --access=public",
    "lib:batch-api:build": "ng-packagr -p libs/batch-api",
    "lib:batch-api:publish": "npm publish ./dist/batch-api --access=public",
    "lib:contact-manager:build": "ng-packagr -p libs/contact-manager",
    "lib:contact-manager:publish": "npm publish ./dist/contact-manager --access=public",
    "lib:datepicker:build": "ng-packagr -p libs/datepicker && cp libs/datepicker/src/bundle.css libs/datepicker/src/_theming.scss dist/datepicker",
    "lib:datepicker:publish": "npm publish ./dist/datepicker --access=public",
    "lib:datetime-picker:build": "ng-packagr -p libs/datetime-picker && mkdir -p dist/datetime-picker/assets/style && node-sass --output-style=compressed libs/datetime-picker/src/sass/picker.scss > dist/datetime-picker/assets/style/picker.min.css",
    "lib:datetime-picker:publish": "npm publish ./dist/datetime-picker --access=public",
    "lib:dealer-api:build": "ng-packagr -p libs/dealer-api",
    "lib:dealer-api:publish": "npm publish ./dist/dealer-api --access=public",
    "lib:firebase-api:build": "ng-packagr -p libs/firebase-api",
    "lib:firebase-api:publish": "npm publish ./dist/firebase-api --access=public",
    "lib:geostore:build": "ng-packagr -p libs/geostore",
    "lib:geostore:publish": "npm publish ./dist/geostore --access=public",
    "lib:interceptors:build": "ng-packagr -p libs/interceptors",
    "lib:interceptors:publish": "npm publish ./dist/interceptors --access=public",
    "lib:safety-api:build": "ng-packagr -p libs/safety-api",
    "lib:safety-api:publish": "npm publish ./dist/safety-api --access=public"
  },
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/fire": "^6.1.5",
    "@angular/forms": "^12.2.16",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@auth0/angular-jwt": "^5.0.2",
    "@digitaldealers/cms-theme": "^0.2.7",
    "@digitaldealers/firebase-api": "^0.4.0",
    "@digitaldealers/geostore": "^0.4.0",
    "@digitaldealers/interceptors": "^0.4.0",
    "@digitaldealers/typings": "^0.3.212",
    "@types/lodash-es": "^4.17.6",
    "@types/uuid": "^8.3.4",
    "angular-2-local-storage": "^3.0.2",
    "date-fns": "^2.28.0",
    "firebase": "^8.10.1",
    "lodash-es": "^4.17.21",
    "ngx-cookie-service": "^12.0.3",
    "ngx-perfect-scrollbar": "^10.1.1",
    "rxjs": "^6.6.7",
    "uuid": "^8.3.2",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^12.2.16",
    "@types/jasmine": "^3.10.3",
    "codelyzer": "^6.0.2",
    "ng-packagr": "^12.2.7",
    "tslint": "^6.1.3",
    "typescript": "~4.3.5"
  }
}
